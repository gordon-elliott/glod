{
  "name": "glod-crudl-graphql",
  "version": "0.3.0",
  "description": "glod crudl (graphql)",
  "main": "index.js",
  "scripts": {
    "browserify": "mkdir -p ./static/admin && browserify . --extension=.jsx > ./static/admin/admin.js",
    "watchify": "mkdir -p ./static/admin && watchify . --extension=.jsx -o ./static/admin/admin.js -v --poll=5000"
  },
  "browserify": {
    "presets": [
      "env",
      "react"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  },
  "browser": "./index.js",
  "author": "Gordon Elliott",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@crudlio/crudl-connectors-base": "^0.1.2",
    "lodash": "^4.17.19",
    "package.json": "^2.0.1",
    "pluralize": "^8.0.0",
    "react": "^16.10.2",
    "uuid": "^3.0.1"
  }
}
